{
    "name": "mrr/mini-strava-api",
    "type": "project",
    "description": "MiniStravaAPI project.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "ext-pdo": "*",
        "blumilksoftware/openapi-toolbox": "^3.0",
        "filament/filament": "^4.0",
        "google-gemini-php/laravel": "^2.0",
        "guzzlehttp/guzzle": "^7.9.3",
        "inertiajs/inertia-laravel": "^2.0.3",
        "intervention/image-laravel": "^1.5",
        "laravel/framework": "^12.20.0",
        "laravel/sanctum": "^4.1.2",
        "laravel/tinker": "^2.10.1",
        "sibyx/phpgpx": "^1.3",
        "yzalis/identicon": "^2.0"
    },
    "require-dev": {
        "blumilksoftware/codestyle": "^v5.0.0",
        "fakerphp/faker": "^1.24.1",
        "larastan/larastan": "^3.5.0",
        "laravel-lang/lang": "^12.17",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.8.2",
        "phpunit/phpunit": "^12.2.7",
        "spatie/laravel-ignition": "^2.9.1"
    },
    "autoload": {
        "psr-4": {
            "Strava\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": [
            "@putenv XDEBUG_MODE=off",
            "@php artisan test"
        ],
        "analyse": "./vendor/bin/phpstan analyse",
        "cs": "./vendor/bin/php-cs-fixer fix --dry-run --diff --config codestyle.php",
        "csf": "./vendor/bin/php-cs-fixer fix --diff --config codestyle.php"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
